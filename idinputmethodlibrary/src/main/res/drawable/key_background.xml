<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

  <item>
    <selector>
      <item android:state_pressed="true">
        <shape>
          <solid android:color="@color/keyboard_key_background_press"/>
        </shape>
      </item>

      <item android:state_pressed="false">
        <shape>
          <solid android:color="@color/keyboard_key_background"/>
        </shape>
      </item>

    </selector>


  </item>

</layer-list>